<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trafficinspecting"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" android:maxSdkVersion="19"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.trafficinspecting.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.statistics.Statistics"
            android:label="@string/app_name"
            >
            </activity>
             <activity
            android:name="com.hardwareinformation.Hardware"
            android:label="@string/app_name"
            ></activity>
             <activity
            android:name="com.aboutme.AboutMe"
            android:label="@string/app_name"
            ></activity>
             <activity
            android:name="com.feedback.Feedback"
            android:label="@string/app_name"
            ></activity>
             <activity
            android:name="com.setting.Setting"
            android:label="@string/app_name"
            ></activity>
              <activity
            android:name="com.testspeed.TestSpeed"
            android:label="@string/app_name"
            ></activity>
    </application>

</manifest>
